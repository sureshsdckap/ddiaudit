<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <authorize_net>
            <anet_webhooks>
                <sender>general</sender>
                <email>recipient@example.com</email>
                <email_template>trans_email_authorize_net_email_template</email_template>
            </anet_webhooks>
        </authorize_net>

        <payment>
            <anet_webhooks>
                <model>AuthorizeNet\Webhooks\Model\Payment\Webhook</model>
            </anet_webhooks>
        </payment>
    </default>
</config>
