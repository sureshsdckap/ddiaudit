<div id="anet_webhooks">
    <button type="button"><span>Create Webhooks</span></button>
    <script type="text/javascript">// <![CDATA[
        require([
                'jquery',
                'Magento_Ui/js/modal/alert'
            ],
            function ($, alert) {
                $('#anet_webhooks').on('click', 'button', function (event) {
                    $('body').trigger('processStart');
                    $.ajax({
                        type: "POST",
                        data: {'test': 'test'},
                        url: '<?= /* @escapeNotVerified */ $block->getUrl('anet_webhooks/create') ?>',
                        success: function (result) {
                            $('body').trigger('processStop');
                            alert({
                                content: result.message
                            });
                        },
                        error: function () {
                            $('body').trigger('processStop');
                            alert({
                                content: '<?= /* @escapeNotVerified */ __('An error occurred') ?>'
                            });
                        },
                        dataType: "json"
                    });
                })
            }
        );
        // ]]></script>
</div>
