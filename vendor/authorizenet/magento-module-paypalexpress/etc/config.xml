<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <payment>
            <anet_paypal_express>
                <active>1</active>
                <model>AuthorizeNetPayPalExpressFacade</model>
                <payment_action>authorize</payment_action>
                <trans_key backend_model="Magento\Config\Model\Config\Backend\Encrypted"/>
                <login_id backend_model="Magento\Config\Model\Config\Backend\Encrypted"/>
                <can_use_checkout>1</can_use_checkout>
                <can_authorize>1</can_authorize>
                <can_capture>1</can_capture>
                <can_refund>1</can_refund>
                <can_void>1</can_void>
                <can_review_payment>1</can_review_payment>
                <can_accept_payment>1</can_accept_payment>
                <can_deny_payment>1</can_deny_payment>
                <can_fetch_transaction_info>0</can_fetch_transaction_info>
                <can_fetch_transaction_information>1</can_fetch_transaction_information>
                <is_gateway>1</is_gateway>
                <is_offline>0</is_offline>
                <title>PayPal Express</title>
                <allowspecific>0</allowspecific>
                <test_mode>0</test_mode>
                <sort_order>1</sort_order>
                <paymentAdditionalInformationKeys>token,payerId,initTransId</paymentAdditionalInformationKeys>
                <paymentInfoKeys>transactionId,payerId,FDSFilterAction,FDSFilters</paymentInfoKeys>
                <privateInfoKeys>transactionId,payerId,FDSFilterAction,FDSFilters</privateInfoKeys>
            </anet_paypal_express>
        </payment>
    </default>
</config>
